{
    "name": "Dissolve",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_title": "Dissolve"
    },
    "icons":{
      "128": "logo.png"
    },
    "permissions" : ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["content-script.js"]
      }
    ]
  }
  